# Example environment variables for AISight
# Copy this file to .env and update the values as needed

# API Configuration
API_URL=https://meri.digitraffic.fi/api/v1

# Map Configuration
MAPLIBRE_STYLE_URL=https://demotiles.maplibre.org/style.json

# MapTiler API key - Get yours at https://www.maptiler.com/cloud/
MAPTILER_API_KEY=your_key_here

# App Environment
ENV=development

# Debug Settings
DEBUG=true
LOG_LEVEL=debug
